<Window x:Class="FinanceSummary.Tests.TestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FinanceSummary.Tests"
        mc:Ignorable="d"
        Title="TestWindow" Height="450" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button Content="Add Transaction Categories" Click="Button_Click"/>
        <Button Grid.Row="1" Content="Add Test Transaction" Click="Button_Click_1"/>
        <Button Grid.Row="2" Content="Add Test Account" Click="Button_Click_2"/>
    </Grid>
</Window>
